                <div id="zoom-outer" style="position: absolute; height: 80%; top:10%;">
                    <div id="zoom-slider" style="height: 80%; left: 7.5%;">
                        <span tabindex="0" id="zoom-handle" class="ui-slider-handle ui-corner-all ui-state-default"></span>
                    </div>
                </div>
            </div>
